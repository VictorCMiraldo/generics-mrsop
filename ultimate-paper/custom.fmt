%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%
%% lhs2TeX Formatting Rules
%%
%
%include stylish.lhs
%
%% Easy to typeset Haskell types using the \HSCon
%% command from stylish.lhs (if it's defined!)
\newcommand{\HT}[1]{\ifdefined\HSCon\HSCon{#1}\else#1\fi}
\newcommand{\HS}[1]{\ifdefined\HSSym\HSSym{#1}\else#1\fi}
\newcommand{\HV}[1]{\ifdefined\HSVar\HSVar{#1}\else#1\fi}
\newcommand{\HK}[1]{\ifdefined\HSKeyword\HSKeyword{#1}\else#1\fi}

% f=wa\HT{f"i}j0
% f=wa\HS{f"i}j0
% f=wa\HV{f"i}j0

%format :*:            = "\mathrel{\HT{:\!*\!:}}"
%format :*             = "\mathbin{\HT{:\!*}}"
%format :+:            = "\mathrel{\HT{:\!+\!:}}"
%format :.:            = "\mathrel{\HT{:\!\circ\!:}}"
%format :@:            = "\mathrel{\HT{:\!\!@\!\!:}}"
%format (P (a))        = "\HS{''}" a
%format Star           = "\HT{*}"
%format GenericSOP     = "\mathit{\HT{Generic^{\mathsf{sop}}}}"
%format All2           = "\mathit{\HT{All_2}}"
%format Generic1       = "\mathit{\HT{Generic_1}}"
%format Generic2       = "\mathit{\HT{Generic_2}}"
%format Rep1           = "\mathit{\HT{Rep_1}}"
%format from1          = "\mathit{\HV{from_1}}"
%format from2          = "\mathit{\HV{from_2}}"
%format to1            = "\mathit{\HV{to_1}}"
%format Par1           = "\mathit{\HT{Par_1}}"
%format Par2           = "\mathit{\HT{Par_2}}"
%format Rec1           = "\mathit{\HT{Rec_1}}"
%format U1             = "\HT{U_1}"
%format V0             = "\HT{V_0}"
%format V2             = "\HT{V_2}"
%format V1             = "\HT{V_1}"
%format M1             = "\HT{M_1}"
%format K1             = "\HT{K_1}"
%format ~              = "\HS{\sim}"
%format GenericRegular = "\mathit{\HT{Generic^{\mathsf{regular}}}}"
%format RepRegular     = "\mathit{\HT{Rep^{\mathsf{regular}}}}"
%format SOP1           = "\mathit{\HT{SOP_1}}"
%format Generic1SOP    = "\mathit{\HT{Generic_1^{\mathsf{sop}}}}"
%format dot            = "\HS{.}"
%format AllRec2        = "\mathit{\HT{AllRec_2}}"
%format verticalspace  = "\vspace{-0.1cm}"
%format SNat           = "\mathit{\HT{SNat}}"
%format LoT            = "\HT{\Gamma}"
%format SLoT           = "\mathit{\HT{S\Gamma}}"
%format dtk            = "\HV{\zeta}"
%format rtk            = "\HV{\rho}"
%format k1             = "\HV{k_1}"
%format k2             = "\HV{k_2}"
%format t1             = "\HV{t_1}"
%format t2             = "\HV{t_2}"
%format tau            = "\HV{\tau}"
%format SZ             = "\mathit{\HT{SZ}}"
%format SS             = "\mathit{\HT{SS}}"
%format LoT0           = "\HT{\epsilon}"
%format :&&:           = "\mathrel{\HT{:\!\!\&\!\!:}}"
%format LookupLoT      = "\HT{LookupIn\Gamma}"
%format GenericNSOP    = "\mathit{\HT{Generic_\star^\mathsf{sop}}}"
%format SOPn           = "\mathit{\HT{SOP_\star}}"
%format SLoT0          = "\HT{S\epsilon}"
%format :&:            = "\mathrel{\HT{:\!\!\text{\&}\!\!:}}"
%format atunder        = "\text{\HS{@}}\HV{\_}"
%format f1             = "\HV{f_1}"
%format fn             = "\HV{f_n}"
%format Set0           = "\mathsf{\HT{Set_0}}"
%format Set1           = "\mathsf{\HT{Set_1}}"
%format Set2           = "\mathsf{\HT{Set_2}}"
%format family         = "\HK{family}"
%format Fixn           = "\mathit{\HT{Fix_\star}}"
%format SKind          = "\mathit{\HT{SKind}}"
%format KK             = "\mathbb{\HT{K}}"
%format ell            = "\HS{\ell}"
%format dots           = "\HS{\dots}"
%format A0             = "\HT{{A_0}}"
%format Arg            = "\HT{{A^{+}}}"
%format unA0           = "\HV{{\mathit{unA}_0}}"
%format unArg          = "\HV{{\mathit{unA}^{+}}}"
%format SSLoT          = "\mathit{\HT{SFor\Gamma}}"
%format sslot          = "\HV{s\Gamma}"
%format SLoTA          = "\mathit{\HT{S\&}}"
%format L1             = "\HT{L_1}"
%format R0             = "\HT{R_0}"
%format R1             = "\HT{R_1}"
%format R2             = "\HT{R_2}"
%format I0             = "\HT{I_0}"
%format I1             = "\HT{I_1}"
%format I2             = "\HT{I_2}"
%format qcolon         = "\mathrel{\HS{''\!:}}"
%format tys            = "\HV{\alpha}"
%format Code1          = "\mathit{\HT{Code_1}}"
%format via            = "\mathbf{via}"
%format TyVar          = "\mathit{\HT{TyVar}}"
%format VZ             = "\mathit{\HT{VZ}}"
%format VS             = "\mathit{\HT{VS}}"
%format as             = "\HV{\alpha}"
%format bs             = "\HV{\beta}"
%format rs             = "\HV{\rho}"
%format ts             = "\HV{\tau}"